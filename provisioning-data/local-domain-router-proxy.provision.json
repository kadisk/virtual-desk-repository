{
    "serviceName": "local-domain-router-proxy",
    "serviceDescription": "Proxy de domínio local é responsável por rotear as requisições para os serviços corretos",
    "repositoryNamespace": "VirtualDeskRepo",
    "packageName": "local-domain-router-proxy",
    "packageType": "app",
    "packagePath": "VirtualDesk.Module/Applications.layer/local-domain-router-proxy.app",
    "startupParams": {
        "entryPort": "7000",
        "routeMappingTable": [
            {
                "host": "kadisk.com.local",
                "target": "http://localhost:7002"
            },
            {
                "host": "worms.solutions.local",
                "target": "http://localhost:7003"
            },
            {
                "host": "virtualdesk.app.local",
                "target": "http://localhost:7001"
            }
        ]
    },
    "ports": [],
    "networkmode": "host"
}
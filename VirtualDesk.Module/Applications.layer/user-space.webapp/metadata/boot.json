{
    "params" : [
        "port", 
        "serverName",
        "serverManagerUrl",
        "RT_ENV_GENERATED_DIR_NAME",
        "isWatch",
        "secretKey",
        "iamStorageSocketPath",
        "iamStorageServerManagerUrl"
    ],
    "services": [
        {
            "namespace": "@@/server-service",
            "dependency": "@/server-manager.service/services/HTTPServerService",
            "params":{
                "name":"{{serverName}}",
                "port": "{{port}}"
            },
            "bound-params":{
                "middlewareService": "@@/jwt-verifier-middleware-service"
            }
        },
        {
            "namespace": "@@/user-management-service",
            "dependency": "@/user-management.service/services/UserManagementService",
            "params":{
                "iamStorageSocketPath": "{{iamStorageSocketPath}}",
                "iamStorageServerManagerUrl": "{{iamStorageServerManagerUrl}}",
                "secretKey":"{{secretKey}}"
            },
            "bound-params":{
                "commandExecutorLib": "@/command-executor.lib"
            }
        },
        {
            "namespace": "@@/jwt-verifier-middleware-service",
            "dependency": "@/server-manager.service/services/JWTVerifierMiddlewareService",
            "params":{
                "secretKey":"{{secretKey}}"
            }
        }
    ],
    "endpoints": [
        {
            "dependency": "@/server-manager.webservice/endpoint-group",
            "bound-params":{
               "serverServiceForController":"@@/server-service",
               "serverService": "@@/server-service"
            }
        },
        {
            "dependency": "@/authenticator.webservice/endpoint-group",
            "params":{
                "needsAuth": true
            },
            "bound-params":{
                "serverService": "@@/server-service",
                "userManagementService": "@@/user-management-service"
            }
        },
        {
            "dependency": "@/user-space.webservice/endpoint-group",
            "params":{
                "needsAuth": true
            },
            "bound-params":{
                "serverService": "@@/server-service"
            }
        },
        {
            "dependency": "@/virtual-desk-user-space.webgui/endpoint-group",
            "params":{
                "serverManagerUrl":"{{serverManagerUrl}}",
                "serverName":"{{serverName}}",
                "RT_ENV_GENERATED_DIR_NAME":"{{RT_ENV_GENERATED_DIR_NAME}}",
                "isWatch": "isWatch"
            },
            "bound-params":{
                "serverService": "@@/server-service"
            }
        },
        {
            "dependency": "@/my-apps-panel.webgui/endpoint-group",
            "params":{
                "serverManagerUrl":"{{serverManagerUrl}}",
                "serverName":"{{serverName}}",
                "RT_ENV_GENERATED_DIR_NAME":"{{RT_ENV_GENERATED_DIR_NAME}}",
                "isWatch": "isWatch"
            },
            "bound-params":{
                "serverService": "@@/server-service"
            }
        },
        {
            "dependency": "@/identity-access-manager.webgui/endpoint-group",
            "params":{
                "serverManagerUrl":"{{serverManagerUrl}}",
                "serverName":"{{serverName}}",
                "RT_ENV_GENERATED_DIR_NAME":"{{RT_ENV_GENERATED_DIR_NAME}}",
                "isWatch": "isWatch"
            },
            "bound-params":{
                "serverService": "@@/server-service"
            }
        }
    ]
}
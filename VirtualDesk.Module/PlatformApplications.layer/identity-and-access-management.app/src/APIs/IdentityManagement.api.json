{
	"name": "IdentityManagement",
	"endpoints": [
		{
			"path": "/organization/:organizationId",
			"method": "GET",
			"summary": "GetOrganization",
			"parameters": [
				{
					"name": "organizationId",
					"in": "path",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"path": "/organization",
			"method": "POST",
			"summary": "CreateOrganization",
			"parameters": [
				{
					"name": "name",
					"in": "body",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"path": "/user",
			"method": "POST",
			"summary": "CreateUser",
			"parameters": [
				{
					"name": "name",
					"in": "body",
					"type": "string",
					"required": true
				},
				{
					"name": "username",
					"in": "body",
					"type": "string",
					"required": true
				},
				{
					"name": "email",
					"in": "body",
					"type": "string",
					"required": true
				},
				{
					"name": "password",
					"in": "body",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"path": "/organization/name",
			"method": "POST",
			"summary": "UpdateOrganizationName",
			"parameters": [
				{
					"name": "organizationId",
					"in": "body",
					"type": "string",
					"required": true
				},
				{
					"name": "name",
					"in": "body",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"path": "/organization/delete",
			"method": "POST",
			"summary": "DeleteOrganization",
			"parameters": [
				{
					"name": "organizationId",
					"in": "body",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"path": "/user/check-exist",
			"method": "POST",
			"summary": "CheckUserExist",
			"parameters": [
				{
					"name": "username",
					"in": "body",
					"type": "string",
					"required": true
				},
				{
					"name": "email",
					"in": "body",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"path": "/user/verify-password",
			"method": "POST",
			"summary": "VerifyPasswordAndGetUser",
			"parameters": [
				{
					"name": "username",
					"in": "body",
					"type": "string",
					"required": true
				},
				{
					"name": "password",
					"in": "body",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"path": "/user/information",
			"method": "POST",
			"summary": "GetUser",
			"parameters": [
				{
					"name": "userId",
					"in": "body",
					"type": "string",
					"required": true
				}
			]
		},
		{
			"path": "/organizations",
			"method": "GET",
			"summary": "ListOrganizations"
		},
		{
			"path": "/users",
			"method": "GET",
			"summary": "ListUsers"
		}
	]
}
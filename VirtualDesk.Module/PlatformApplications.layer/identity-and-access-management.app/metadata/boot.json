{
    "params" : [
        "storageFilePath",
        "serverName",
        "socket"
    ],
    "services": [
        {
            "namespace": "@@/server-service",
            "dependency": "@/server-manager.service/services/HTTPServerService",
            "params":{
                "name":"{{serverName}}",
                "port": "{{socket}}"
            }
        },
        {
            "namespace": "@@/iam-manager-service",
            "dependency": "@//services/IdentityAndAccessManager",
            "params":{
                "storageFilePath": "{{storageFilePath}}"
            }
        }
    ],
    "endpoints": [
        {
            "dependency": "@/server-manager.webservice/endpoint-group",
            "bound-params":{
               "serverServiceForController":"@@/server-service",
               "serverService": "@@/server-service"
            }
        },
        {
            "dependency": "@//endpoint-group",
            "bound-params":{
                "serverService": "@@/server-service",
                "iamManagerService": "@@/iam-manager-service"
            }
        }
    ]
}